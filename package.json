{
  "name": "AllCoinStat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open --host 21184.c.hostens.cloud",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "highcharts": "^9.0.0",
    "normalize.css": "^8.0.0",
    "v-tooltip": "^2.0.0-rc.31",
    "vue": "^2.5.13",
    "vue-meta": "^1.4.3",
    "vue-multiselect": "^2.0.8",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.9.3",
    "css-loader": "^3.1.0",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.9.0",
    "postcss-html": "^0.14.0",
    "postcss-import": "^11.1.0",
    "postcss-nesting": "^4.2.1",
    "postcss-preset-env": "^3.2.2",
    "postcss-reporter": "^5.0.0",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "vue": {
    "lintOnSave": "error"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "vue/max-attributes-per-line": "off",
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off"
    }
  },
  "postcss": {
    "plugins": {
      "postcss-html": {},
      "postcss-import": {},
      "postcss-nesting": {},
      "postcss-preset-env": {},
      "autoprefixer": {},
      "postcss-reporter": {
        "clearReportedMessages": true,
        "throwError": true,
        "plugins": [
          "postcss-import"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
